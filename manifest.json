{
    "manifest_version": 3,
    "name": "tAi nAkE Emoji Extension",
    "version": "1.0",
    "description": "A Chrome extension that adds emojis to your text.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": ["clipboardWrite"]
  }
  